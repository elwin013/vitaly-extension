{
  "name": "vitaly-extension",
  "description": "A starter template for Chrome Extension (MV3) with Vite, Vue and Typescript",
  "private": true,
  "version": "1.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "package": "npm run build && node build.js",
    "test": "vitest",
    "lint": "yarn run eslint --ext .js,.ts,.vue,.jsx src/",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx,json,css,scss,md,vue}'"
  },
  "dependencies": {
    "vue": "^3.2.47",
    "webextension-polyfill": "^0.10.0"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "2.0.0-beta.12",
    "@types/chrome": "^0.0.213",
    "@types/jest": "^29.4.0",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "crx3": "^1.1.3",
    "eslint": "^8.34.0",
    "eslint-plugin-vue": "^9.9.0",
    "prettier": "^2.8.4",
    "sass": "^1.58.1",
    "typescript": "^4.9.5",
    "vite": "3.2.5",
    "vitest": "^0.28.5",
    "vue-tsc": "^1.0.24"
  },
  "engines": {
    "node": ">= 18.14.0",
    "npm": ">= 8.11.0",
    "yarn": ">= 1.22.0"
  }
}